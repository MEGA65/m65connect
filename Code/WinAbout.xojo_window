#tag WindowCode
	#tag Event
		Sub Open()
		  // Default layout is optimized for Linux, adjust layout for Windows and Mac
		  #If TargetWindows Or TargetMacOS Then
		    PushClose.Height = 22
		  #EndIf
		  
		  // Set window position
		  WinAbout.Left = (MainWindow.Left + MainWindow.Width) - ((WinAbout.Width + MainWindow.Width) /2)
		  WinAbout.Top = (MainWindow.Left + MainWindow.Height) - ((WinAbout.Height + MainWindow.Height) /2)
		End Sub
	#tag EndEvent


#tag EndWindowCode

#tag Events PushClose
	#tag Event
		Sub Action()
		  // Close window
		  WinAbout.Close
		End Sub
	#tag EndEvent
#tag EndEvents
